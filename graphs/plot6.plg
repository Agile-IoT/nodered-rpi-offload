set term png size 640,240
set datafile separator ","
set title title offset 0,-16
set key autotitle columnhead

set grid

set xlabel 'job ID'
set ylabel 'job duration [s]'
set xrange[1:njobs]
#set yrange[0:]

set style line 1 \
    pointtype 3 pointsize 1

plot csv using 4:($8/1000) with points linestyle 1 notitle
